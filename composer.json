{
    "name": "dimimo/breadcrumbs",
    "description": "A simple Laravel-style way to create breadcrumbs.",
    "keywords": [
        "laravel"
    ],
    "homepage": "https://github.com/dimimo/breadcrumbs",
    "authors": [
        {
            "name": "Dave James Miller",
            "email": "dave@davejamesmiller.com"
        },
        {
            "name": "Dimitri Mostrey",
            "email": "dmostrey@yahoo.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "facade/ignition-contracts": "^1.0",
        "laravel/framework": "^6|^7|^8"
    },
    "require-dev": {
        "orchestra/testbench": "^6|^7",
        "phpunit/phpunit": "^7.0|^8.0",
        "php-coveralls/php-coveralls": "^2.0",
        "spatie/phpunit-snapshot-assertions": "^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dimimo\\Breadcrumbs\\": "classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/",
            "BreadcrumbsTests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dimimo\\Breadcrumbs\\BreadcrumbsServiceProvider"
            ],
            "aliases": {
                "Breadcrumbs": "Dimimo\\Breadcrumbs\\Facades\\Breadcrumbs"
            }
        }
    }
}
